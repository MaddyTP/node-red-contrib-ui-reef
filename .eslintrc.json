{
    "env": {
        "commonjs": true,
        "es2017": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "max-len": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "no-nested-ternary": 0,
        "prefer-template": 0,
        "no-prototype-builtins": 0,
        "prefer-destructuring": 0,
        "camelcase": "warn",
        "no-shadow": "warn",
        "no-use-before-define": "warn",
        "no-undef": "warn",
        "no-restricted-syntax": 0,
        "no-unused-vars": "warn",
        "consistent-return": "warn",
        "no-unused-expressions": "warn",
        "object-shorthand": 0,
        "func-names": 0,
        "prefer-arrow-callback": 0
    },
    "plugins": [
        "eslint-plugin-html"
    ]
}
